@model HomeViewModel;

@{
    ViewData["Title"] = "Home Page";
}

<div class="row">
    <div class="col-md-3">
        <h2>Persons</h2>
        <ul>
            @foreach (var person in Model.Persons)
            {
                <li>Name: @person.Name, Age: @person.Age.ToString()</li>
            }
        </ul>
    </div>
</div>
